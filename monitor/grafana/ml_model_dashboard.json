{
  "dashboard": {
    "id": null,
    "title": "ML Model Monitoring Dashboard",
    "description": "Complete monitoring dashboard for ML model inference service",
    "tags": ["machine-learning", "monitoring", "inference"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "üéØ Total Predictions",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "ml_predictions_total",
            "legendFormat": "Total Predictions",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "‚è±Ô∏è Prediction Latency",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(ml_prediction_latency_seconds_sum[5m]) / rate(ml_prediction_latency_seconds_count[5m])",
            "legendFormat": "Average Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ml_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "alert": {
          "alertRuleTags": {},
          "conditions": [
            {
              "evaluator": {
                "params": [0.5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "1m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Prediction Latency",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "üéØ Model Confidence Score",
        "type": "gauge",
        "gridPos": {
          "h": 8,
          "w": 9,
          "x": 15,
          "y": 0
        },
        "targets": [
          {
            "expr": "ml_model_confidence_score",
            "legendFormat": "Confidence",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 1,
            "min": 0,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "green",
                  "value": 0.8
                }
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 4,
        "title": "üìä API Requests by Status",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{status}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 5,
        "title": "‚ö†Ô∏è Error Rate",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "api_error_rate",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 6,
        "title": "üë• Active Users",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "targets": [
          {
            "expr": "api_active_users",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "blue",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "üíæ System Resources",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "api_cpu_usage_percent",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "api_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage MB",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage / MB",
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "üîî Recent Alerts",
        "type": "logs",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "{job=\"inference-service\"}",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1,
    "links": []
  }
}